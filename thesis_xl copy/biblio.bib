	Keywords = {{temporal structures; descriptive methods; correlation; manual; applications; regression; spatial structure; testing; estimation; bootstrap methods; tables of probability distributions; circular data; entomology; ornithology; herpetology; geology; meteorology; examples}},
	Language = {English},
