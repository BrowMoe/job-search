@article{BarronBirgeMassart1999,
	Abstract = {{The authors develop performance bounds for criteria of model
    selection, using recent theory for sieves. The model selection criteria are
    based on an empirical loss or contrast function with an added penalty term
    roughly proportional to the number of parameters needed to describe the
    model divided by the number of observations. Most of the presented examples
    involve density or regression estimation settings, and the authors focus on
    the problem of estimating the unknown density or regression function.\par It
    is shown that the quadratic risk of the minimum penalized empirical contrast
    estimator is bounded by an index of the accuracy of the sieve. The
    connection between model selection via penalization and adaptation in the
    minimax sense is pointed out. Such illustrations of the introduced method as
    penalized maximum likelihood, projection or least squares estimation are
    provided. The models involve commonly used finite dimensional expansions
    such as piecewise polynomials with fixed or variable knots, trigonometric
    polynomials, wavelets, neural nets, and related nonlinear expansions defined
    by superposition of ridge functions.}},
	Author = {Barron, Andrew and Birg\'e, Lucien and Massart, Pascal},
	Classmath = {{*62G05 (Nonparametric estimation) 62G07 (Curve estimation) 41A25 (Degree of approximation, etc.) }},
	Date-Added = {2019-03-10 15:15:11 +0000},
	Date-Modified = {2019-03-10 15:15:11 +0000},
	Journal = {Probability Theory and Related Fields},
	Keywords = {{sieves; adaptation; penalized maximum likelihood; projection; least squares estimation}},
	Language = {English},
	Number = {3},
	Pages = {301-413},
	Reviewer = {{Joseph Melamed (Los Angeles)}},
	Title = {{Risk bounds for model selection via penalization.}},
	Volume = {113},
	Year = {1999}}

@book{Fisher1993,
	Abstract = {{This book is a companion volume to the former book of the present
    author, {\it T. Lewis} and {\it B. J. J. Embleton}, ``Statistical analysis
    of spherical data.'' (1987; Zbl 0651.62045), but includes some newer
    techniques. It is primarily a manual for the working scientist, with the
    emphasis on applications. The level of mathematics required to use the book
    would be achieved by a first-year mathematics course. The emphasis is on
    statistical analysis of data, rather than on statistical theory.\par After
    an introduction, the chapter headings are: Descriptive methods; Models;
    Analysis of a single sample of data; Analysis of two or more samples, and of
    other experimental layouts; Correlation and regression; Analysis of data
    with temporal or spatial structure; Some modern statistical techniques for
    testing and estimation. Among the techniques covered in the last chapter are
    bootstrap methods.\par One appendix contains various statistical tables of
    probability distributions used in the analysis of circular data. A second
    appendix contains 24 data sets, drawn from various fields, including
    entomology, ornithology, herpetology, geology, meteorology, and others.
    Since this is not meant to be a textbook, there are no exercises, but there
    are many examples worked out in detail.}},
	Author = {Fisher, N.I.},
	Classmath = {{*62H11 (Directional data; spatial statistics) 62-01 (Textbooks (statistics)) 62-02 (Research monographs (statistics)) 62-07 (Data analysis (statistics)) 62Q05 (Statistical tables) }},
